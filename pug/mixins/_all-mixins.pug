mixin field-select(title, placeholder, ...icons)
    .field
        .field__title
            h3= title
            -if (icons)
                .field__title-buttons
                    each icon in icons
                        .button-icon(class=`button-icon--${icon}`)
        .field__box
            .field__box-select
                select
                    option 1
                    option 2
                    option 3
                    option 4
                    option 5


mixin field-textarea(title, placeholder)
    .field
        .field__title
            h3= title
        .field__box
            textarea(placeholder=placeholder)

mixin field-radio(title, placeholder1, placeholder2)
    .field
        .field__title
            h3= title
        .field__box
            .field__box-wrap
                input(type='radio' id='id1-1' name='id1' checked)
                label(type='radio' for='id1-1')= placeholder1
            .field__box-wrap
                input(type='radio' id='id1-2' name='id1')
                label(type='radio' for='id1-2')= placeholder2

