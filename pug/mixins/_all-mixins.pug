mixin field-input(title, placeholder, ...icons)
    .field
        .field__title
            h3= title
            -if(icons)
                .field__title-buttons
                    each icon in icons
                        a(href=`#popup-${icon}`).button-icon(class=`button-icon--${icon}`).js-show-popup

        .field__box
            input(type='text' placeholder=placeholder)

mixin field-input-cost(title, placeholder)
    .field
        .field__title
            h3= title
            .field__title-buttons
                .button-icon.button-icon--green-plus.js-show-tooltip(data-type='add')
                .button-icon.button-icon--equal.js-show-tooltip(data-type='add')
                .button-icon.button-icon--delete.js-show-tooltip(data-type='remove')

        .field__box
            input(type='text' placeholder=placeholder)

mixin field-input-cost-refresh(title, placeholder)
    .field
        .field__title
            h3= title
            .field__title-buttons
                .button-icon.button-icon--green-plus.js-show-tooltip(data-type='add')
                .button-icon.button-icon--equal.js-show-tooltip(data-type='add')
                .button-icon.button-icon--delete.js-show-tooltip(data-type='remove')
                .button-icon.button-icon--refresh-gradient

        .field__box
            input(type='text' placeholder=placeholder)

mixin field-input-collapsed(title)
    .field
        .field__title
            h3= title

        .field__box
            .field__box-table
                .custom-table.custom-table--collapsed.custom-table--single
                    table.js-table-collapsed
